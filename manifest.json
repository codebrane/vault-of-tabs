{
	"manifest_version": 2,
	"version": "0.0.4",
	"name": "Vault of Tabs",
	"author": "Alistair Young",
	"description": "Saves your tabs to a JSON file in your downloads folder",
	"developer": {
	  "name": "Alistair Young",
	  "url": "http://codebrane.com"
	},
  "icons": {
    "16": "icons/icon-16.png",
		"32": "icons/icon-32.png",
		"64": "icons/icon-64.png"
  },
	"background": {
	  "scripts": ["index.js", "moment.min.js"]
	},
	"browser_action": {
	  "default_icon": "icons/icon-32.png"
	},
	"permissions": [
    "tabs",
		"downloads"
	]
}
